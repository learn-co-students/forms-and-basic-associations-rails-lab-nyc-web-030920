<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name%>
  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <% @song.notes.each do |note|%>
  <%= f.label :note %>
  <%= f.text_field :note%>
  <% end %>
  <br />

  <%= f.submit %>
<% end %>


